function OnChange(e)
{
	var gubun = e;    
	switch(gubun)    
	{
	    case "in":      
		{ 
		 document.getElementById('in_time').innerHTML = "09:00";
		 break;
		 }
		 case "out":
		 {
		  document.getElementById('out_time').innerHTML = "18:00";
		  break;
		  }
	}
}

function showModal(){
